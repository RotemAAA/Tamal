        app:layout_constraintEnd_toStartOf="@+id/gv10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gh20"
        app:layout_constraintTop_toTopOf="@+id/guideline69"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/etFoodCapacity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ems="10"
        android:hint="@string/food_capacity"
        android:background="@drawable/asset114"
